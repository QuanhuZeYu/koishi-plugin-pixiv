{
  "name": "@quanhuzeyu/koishi-plugin-pixiv",
  "description": "与pixiv相关的操作插件，无需获取cookies等复杂操作，获取账号下的每日推荐",
  "version": "0.1.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "clean": "node clean.js",
    "allpub": "npm run pubgit && npm run pubnpm",
    "pubgit": "npm publish --registry=https://npm.pkg.github.com --access=public",
    "pubnpm": "npm publish --registry=https://registry.npmjs.org --access=public"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin"
  ],
  "devDependencies": {
    "koishi-plugin-puppeteer": "^3.9.0",
    "puppeteer-core": "^23.3.0"
  },
  "peerDependencies": {
    "koishi": "^4.17.11",
    "koishi-plugin-puppeteer": "^3.9.0"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
